<!DOCTYPE html>
<html>
  <head>
    <title>Input Form</title>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <script type="text/javascript" src="/routes/home.js"></script>
    <script src="/vendor/jquery-3.0.0.js"></script>
    <script defer src="/scripts/app.js"></script>
  </head>
  <body>
    <form method="POST" action="/home/new">
      <label for="user_id">User ID:</label>
      <input type="text" id="user_id" name="user_id" /><br /><br />
      <label for="cat_id">Cat ID:</label>
      <input type="text" id="cat_id" name="cat_id" /><br /><br />
      <label for="priority">Priority:</label>
      <input type="text" id="priority" name="priority" /><br /><br />
      <label for="title">Title:</label>
      <input type="text" id="title" name="title" /><br /><br />
      <label for="task_due">Task Due:</label>
      <input type="text" id="task_due" name="task_due" /><br /><br />
      <input type="submit" value="Submit" />
    </form>
  </body>
</html>
